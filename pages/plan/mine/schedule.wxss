/* pages/subscribe/mine/schedule.wxss */
@import "../index.wxss";
page{
  background: #F2F2F2;
}

.list{
  background: #fff;
  margin-bottom: 10rpx;
  padding: 30rpx;
}
.latest{
  padding: 20rpx;
  height:34rpx;
  font-size:24rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
  line-height:34rpx;
  text-align: center;
}

.info{
  padding-bottom: 18rpx;
  margin-bottom: 30rpx;
}
.info .item1{
  font-size:32rpx;
  font-weight:600;
  color:rgba(0,0,0,1);
  line-height:44rpx;
  margin-bottom: 20rpx;
}
.info .item2 text{
  display: block;
  float: right;
  width:80rpx;
  height:34rpx;
  line-height: 34rpx;
  text-align: center;
  background:rgba(238,238,238,1);
  border-radius:2rpx;
  font-size:18rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.info .item2 text.status2{
  background: #000000;
  color: #FFFFFF;
}
.info .item2 text.status3{
  background: #E50113;
  color: #FFFFFF;
}

.info .item2 text.status4{
  background: #07c160;
  color: #fff;
}

.info .item2{
  font-size:22rpx;
  font-weight:400;
  color:rgba(102,102,102,1);
  line-height:32rpx;
}
.contact{
  border-top: 2rpx solid #EEEEEE;
  font-size:24rpx;
  font-weight:400;
  color:rgba(0,0,0,1);
  line-height:34rpx;
  padding: 30rpx 0 10rpx;
}
.contact view{
  float: right;
}
.contact view text{
  display: inline-block;
  vertical-align: middle;
}
.contact image{
  vertical-align: middle;
  margin-left: 10rpx;
}