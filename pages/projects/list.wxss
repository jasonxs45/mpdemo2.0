page {
  background: #000;
}
.control-group.fixed {
  position: fixed;
  width: 100%;
  left: 0;
}
.control-group{
  z-index: 2;
}

.cover {
  position: relative;
  width: 100%;
  height: 250rpx;
  background: #000;
}
.cover.high{
  height: 690rpx;
}
.cover.low{
  height: 250rpx;
}

.cover .img {
  width: 100%;
  height: 100%;
}

.cover .gradient {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.1) 50%, rgba(0, 0, 0, 1) 100%);
}

.top-bar {
  position: relative;
  background: rgba(0,0,0,.8);
  align-items: center;
  padding: 0 40rpx;
  height: 74rpx;
  margin-top: -74rpx;
}

.top-bar .op {
  border: 1px solid #fff;
  border-radius: 8rpx;
  overflow: hidden;
  flex: 1;
}

.top-bar .map-nav {
  background: none;
  width: 110rpx;
  height: 70rpx;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 1);
  padding:0;
  box-sizing: border-box;
  text-align: center;
  margin-left: 10rpx;
}
.top-bar .map-nav text,
.top-bar .map-nav .iconfont{
  display: inline-block;
  vertical-align: top;
  line-height: 70rpx;
  padding-top: 0;
}
.top-bar .map-nav .iconfont{
  margin-left: 6rpx;
}
.top-bar .map-nav text{
  line-height: 1;
  padding-top: 13rpx;
}

.top-bar .nav-bar {
  background: rgba(0,0,0,.8);
  padding:0 15rpx 0 0;
}

.top-bar .search-bar {
  background: rgba(0,0,0,.8);
}
.top-bar .search-bar .search-input{
  height: 70rpx;
}
.top-bar .search-bar .search-input .inner-input{
  margin: 0 15rpx;
}
.top-bar .nav-bar .location{
  margin-left: 0;
  height: 70rpx;
  line-height: 70rpx;
}

.filter-bar {
  background: rgba(0,0,0,.8);
  position: relative;
}

.bg {
  position: fixed;
  width: 100%;
  left: 0;
  top: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .8);
  z-index: 1;
}

.filter-item {
  flex: 1;
  font-size: 28rpx;
  color: #fff;
  padding: 5rpx 15rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
}

.filter-item.active {
  color: rgba(var(--r), var(--g), var(--b), 1);
}

.filter-item .text {
  display: inline-block;
  max-width: 130rpx;
  padding-right: 10rpx;
}

.filter-item .text.active {
  color: rgba(var(--r), var(--g), var(--b), 1);
}

.filter-item .icon {
  color: #fff;
  font-size: 20rpx;
  transition: all 0.3s;
}

.filter-item.active .icon {
  color: rgba(var(--r), var(--g), var(--b), 1);
  transform: rotate(180deg);
}

.filter-detail-wrapper {
  position: absolute;
  z-index: 2;
  width: 100%;
  height: 500rpx;
  left: 0;
  top: 100%;
  padding: 15rpx 0 0;
  background: rgba(0,0,0,.9);
}

.filter-detail-wrapper .btns {
  height: 100rpx;
  align-items: center;
  justify-content: space-around;
  padding: 0 30rpx;
}

.filter-detail-wrapper .btn {
  width: 210rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0;
  margin: 0;
  font-size: 28rpx;
}
.filter-detail-wrapper .btn.reset{
  background: none;
  border: 1px solid rgba(var(--r), var(--g), var(--b), 1);
  color: rgba(var(--r), var(--g), var(--b), 1);
}

.filter-detail-scroll-view {
  height: calc(100% - 100rpx);
  width: 100%;
}

.brief-info {
  height: 50rpx;
  width: 100%;
  line-height: 50rpx;
  font-size: 24rpx;
  padding: 0 40rpx;
  background: rgba(0, 0, 0, .8);
  box-shadow: 0 4rpx 4rpx -4rpx rgba(0, 0, 0, 0.2);
  position: relative;
  z-index: 1;
}

.brief-info {
  align-items: center;
}

.brief-info .desc {
  width: calc(100% - 100rpx);
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #888;
}

.brief-info .desc .txt {
  color: rgba(var(--r), var(--g), var(--b), 1);
}

.brief-info .clear {
  width: 100rpx;
  height: 100%;
  line-height: 50rpx;
  text-align: center;
  color: #fff;
  font-weight: 500;
}
.main-scroll-view {
  /* height: calc(100vh - 190rpx); */
  padding-bottom: 152rpx;
  padding-bottom: calc(152rpx + constant(safe-area-inset-bottom));
  padding-bottom: calc(152rpx + env(safe-area-inset-bottom));
}
.control-group.fixed + .main-scroll-view{
  padding-top: 780rpx;
}

.control-group.fixed + .main-scroll-view.short {
  padding-top: 810rpx;
}

.list-container {
  padding-top: 0 !important;
}

.nav {
  padding: 30rpx;
  position: relative;
}
.nav:before{
  content: '';
  display: block;
  position: absolute;
  width: 6rpx;
  height: 100rpx;
  background-color: rgba(var(--r), var(--g), var(--b), 1);
  left: 0;
  top:30rpx;
}
.nav .nav-wrapper{
  width: 670rpx;
  margin: 0 auto;
}
.nav .title{
  font-size:42rpx;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.nav .addr{
  font-size:22rpx;
  letter-spacing: 3rpx;
  color:rgba(85,85,85,1);
}
.nav .tags{
  margin: 12rpx -8rpx;
  font-size: 0;
}
.nav .tag{
  display: inline-block;
  vertical-align: top;
  margin: 8rpx;
  border:1px solid rgba(var(--r), var(--g), var(--b), 1);
  font-size:18rpx;
  color:rgba(var(--r), var(--g), var(--b), 1);
  line-height:30rpx;
  padding: 5rpx 10rpx;
}
.nav .thumbnail{
  width: 100%;
  margin: 35rpx 0;
  position: relative;
}
.nav .thumbnail .img{
  width: 100%;
}
.nav .thumbnail .info{
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  justify-content: space-between;
  align-items: flex-end;
  background: rgba(0, 0, 0, .6);
}
.nav .thumbnail .focus{
  font-size:22rpx;
  color:rgba(255,255,255,1);
  padding: 20rpx
}
.nav .thumbnail .price{
  font-size:22rpx;
  color:rgba(255,255,255,1);
  padding: 20rpx;
}
.nav .thumbnail .price .txt{
  font-size: 36rpx;
  color:rgba(221, 167, 86, 1);
}
.select-group {
  width: 100%;
}

.select-group.cell {
  width: auto;
  margin-left: 15rpx;
  margin-right: 15rpx;
}

.select-label {
  box-sizing: border-box;
  display: block;
  width: 100%;
  height: 90rpx;
  border-top: 1rpx solid #eee;
}

.select-label:first-child {
  border-top: none;
}

.select-group.cell .select-label {
  display: inline-block;
  width: 33.33%;
  height: auto;
  padding: 15rpx;
  border-top: none;
}

.select-tar {
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  position: relative;
  font-size: 24rpx;
  padding: 0 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #fff;
  justify-content: space-between;
  align-items: center;
}

.select-tar .icon {
  opacity: 0;
}

.select-group.cell .select-tar {
  border: 1px solid #ddd;
  text-align: center;
  border-radius: 4rpx;
}

.select-tar.active {
  color: rgba(var(--r), var(--g), var(--b), 1);
  background: rgba(var(--r), var(--g), var(--b), 0.05);
  font-weight: 600;
}

.select-tar.active .icon {
  opacity: 1;
}

.plist-tab {
  background: #000 !important;
  color: #fff;
}
.plist-tab .weui-tabbar__label{
  color: #fff;
}
.no-data .img{
  opacity: .5;
}